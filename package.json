{
  "name": "Banner_factory",
  "version": "1.0.0",
  "description": "banners boilerplate",
  "repository": "https://github.com/z80b/banners.git",
  "scripts": {
    "start": "npm run watch & npm run server",
    "build": "./node_modules/.bin/rollup -c --environment BUILD:production",
    "dev": "./node_modules/.bin/rollup -c --environment BUILD:development",
    "watch": "./node_modules/.bin/rollup -c -w --environment BUILD:development",
    "server": "node ./server.js",
    "build:css": "node ./build/css.js --input=src/css/index.styl --output=tmp/index.css",
    "build:html": "node ./build/html.js"
  },
  "author": "Stitch",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "axis": "^1.0.0",
    "backbone": "^1.4.0",
    "jquery": "^3.4.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.1",
    "autoprefixer-stylus": "^1.0.0",
    "babel-runtime": "^6.26.0",
    "cheerio": "^1.0.0-rc.3",
    "curl-request": "^1.1.1",
    "express": "^4.17.1",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-import-alias": "^1.0.10",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-stylus-compiler": "^1.0.1",
    "rollup-plugin-template-html": "0.0.3",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-underscorify": "^1.0.0",
    "rollup-watch": "^4.3.1",
    "unzip": "^0.1.11",
    "yargs": "^15.0.2"
  }
}
